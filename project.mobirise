ass=\"user-box mbr-flex mbr-jc-c mbr-pt-4 mbr-pb-3\">\n                    <div class=\"card-img img-with-overlay\">\n                        <amp-img class=\"mobirise-loader\" mbr-class=\"{'rounded': roundedImage}\" src=\"@PROJECT_PATH@/assets/images/face4.jpg\" layout=\"responsive\" width=\"80\" height=\"80\" alt=\"Mobirise\">\n                            <div placeholder=\"\" class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/face4.jpg\" alt=\"Mobirise\">\n                        </amp-img>\n                    </div>\n                    <div>\n                        <h3 class=\"card-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title\">Jane Olsen</h3>\n                        <h4 class=\"cards-subtitle mbr-regular mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showCardSubtitle\" data-app-selector=\".cards-subtitle\">User</h4>\n                    </div>\n                </div>\n            </div>\n            <!--  -->\n            <div class=\"mbr-col-sm-12\" mbr-class=\"{\n                             'mbr-col-md-8 mbr-col-lg-4': cardsAmount < 4,\n                             'mbr-col-md-6 mbr-col-lg-3':cardsAmount == 4,\n                             'align-center':cardAlign==2,\n                             'align-right':cardAlign==3,\n                             'last-child': cardsAmount == 1}\" mbr-if=\"cardsAmount>3\">\n                <div class=\"card\">\n                    <div class=\"card-wrapper\">\n\n                        <span mbr-icon=\"\" class=\"quote fa-quote-left fa\" mbr-if=\"showSocial\"></span>\n\n                        <div class=\"card-box\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showSocial\">\n                            <p class=\"card-text mbr-fonts-style mbr-light\" mbr-theme-style=\"display-7\" mbr-if=\"showCardText\" data-app-selector=\".card-text\">Lorem ip